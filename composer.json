{
    "name": "shahrukh/payments",
    "description": ":description",
    "authors": [
        {
            "name": "Shahrukh Anwar",
            "email": "shahrukh.anwar@hestabit.in",
            "homepage": "https://github.com/shahrukh4"
        }
    ],
    "homepage": "https://github.com/shahrukh/payments",
    "keywords": ["Laravel", "Payments", "PayPal", "Stripe", "Payment Gateway", "Multi Payment Gateways"],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5",
        "cartalyst/stripe-laravel": "7.0.*",
        "anouar/paypalpayment": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shahrukh\\Payments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahrukh\\Payments\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahrukh\\Payments\\PaymentsServiceProvider"
            ],
            "aliases": {
                "Payments": "Shahrukh\\Payments\\Facades\\Payments"
            }
        }
    }
